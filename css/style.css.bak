/* ----------------------------
   single popup styles
   ----------------------------*/

/* first style */
.popup.one {
   top: 0;
   left: 0;
   -webkit-transform: scale(0);
   transform: scale(0);
   opacity: 0;
   transition: all .7s cubic-bezier(0.845, -0.265, 0.190, 1.280);
}
   .popup.oneOpen {
      -webkit-transform: scale(1);
      transform: scale(1);
      opacity: 1
   }



