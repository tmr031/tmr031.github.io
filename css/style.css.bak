.agree{
  display: block;
  position: relative;
  margin: 40px auto;
  height: auto;
  width: 30%;
  margin-left:35%;
}


.agree ul{
  list-style: none;
  margin: 0;
  padding: 0;
	overflow: auto;
}

.agree ul li{
  color: #525252;
  display: inline-block;
  position: relative;
  float: left;
  width: 50%;
  height: 100px;
}

.agree ul li input[type=radio]{
  position: absolute;
  visibility: hidden;
}

.agree ul li label{
  display: block;
  position: relative;
  font-weight: 300;
  font-size: 1.35em;
  padding: 25px 25px 25px 80px;
  margin: 10px auto;
  height: 30px;
  z-index: 9;
  cursor: pointer;
  -webkit-transition: all 0.25s linear;
}

.agree ul li:hover label{
	color: #bcbcbc;
}

.agree ul li .check{
  display: block;
  position: absolute;
  border: 5px solid #525252;
  border-radius: 100%;
  height: 25px;
  width: 25px;
  top: 30px;
  left: 20px;
	z-index: 5;
	transition: border .25s linear;
	-webkit-transition: border .25s linear;
}

.agree ul li:hover .check {
  border: 5px solid #bcbcbc;
}

.agree ul li .check::before {
  display: block;
  position: absolute;
	content: '';
  border-radius: 100%;
  height: 15px;
  width: 15px;
  top: 5px;
	left: 5px;
  margin: auto;
	transition: background 0.25s linear;
	-webkit-transition: background 0.25s linear;
}

.agree input[type=radio]:checked ~ .check {
  border: 5px solid #000;
}

.agree input[type=radio]:checked ~ .check::before{
  background: #000;
}

.agree input[type=radio]:checked ~ label{
  color: #000;
}


