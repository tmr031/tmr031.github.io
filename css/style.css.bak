.mySlides {display:none;}



.prev, .next {

  cursor: pointer;

  position: absolute;

  top: 50%;

  width: auto;

  color:#fff;
  padding: 8px 20px;

  margin-top: -22px;

  font-weight: bold;

  font-size: 30px;

  transition: 0.6s ease;

  margin-left:2%; margin-right:2%;

}



/* Position the "next button" to the right */

.next {

  right: 0;

  border-radius: 3px 0 0 3px;

}



/* On hover, add a black background color with a little bit see-through */

.prev:hover, .next:hover {

  background-color: rgba(0,0,0,0.8);

  color:#fff;

}



/* Caption text */

.text {

  color: #f2f2f2;

  font-size: 15px;

  padding: 8px 12px;

  position: absolute;

  bottom: 8px;

  width: 100%;

  text-align: center;

}



/* Number text (1/3 etc) */

.numbertext {

  color: #f2f2f2;

  font-size: 12px;

  padding: 8px 12px;

  position: absolute;

  top: 0;

}

.fade {

  -webkit-animation-name: fade;

  -webkit-animation-duration: 1.5s;

  animation-name: fade;

  animation-duration: 1.5s;

}



@-webkit-keyframes fade {

  from {opacity: .4} 

  to {opacity: 1}

}



@keyframes fade {
  from {opacity: .4} 
  to {opacity: 1}
}







