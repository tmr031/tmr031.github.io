#suitepic2 .captionbox, #suitepic1 .captionbox {
position:relative;
  top: 20px;
  left: 20px;
  right: 20px;
  bottom: 20px;
  border: 4px solid white;
  border-width: 0 4px;
}

.bigcaption {
  overflow: hidden;
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
}

.caption {
  overflow: hidden;
  -webkit-transform: translateY(50%);
  transform: translateY(50%);
  position: absolute;
  width: 100%;
  bottom: 0;
}

.captionbox h3,
.captionbox p {
  display: table;
  margin: 0 auto; color:#fff;
  padding: 0 10px;
  font-size:2em;
  position: relative;
  text-align: center;
  width: auto;
  text-transform: uppercase;
  font-weight: 400;
}

.captionbox h3 span,
.captionbox p span {
  font-weight: 800;
}


.captionbox h3:before,
.captionbox p:before,
.captionbox h3:after,
.captionbox p:after {
  position: absolute;
  display: block;
  width: 1000%;
  height: 4px;
  content: '';
  background: white;
}

.captionbox h3:before,
.captionbox p:before {
  left: -1000%;
}

.captionbox h3:after,
.captionbox p:after {
  right: -1000%;
}

.captionbox h3:before,
.captionbox h3:after {
  top: 50%;
}

.captionbox p {
  font-size: 1em;
  font-weight: 600;
}

.captionbox p:before,
.captionbox p:after {
  bottom: 50%;
}

.captionbox {display:none;}


